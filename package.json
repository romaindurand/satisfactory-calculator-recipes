{
  "name": "satisfactory-calculator-recipes",
  "description": "An extention for Satisfactory Calculator that checks recipes you unlocked so you don't have to.",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
